{
  "name": "odyssey-scrollyteller",
  "version": "2.0.0",
  "description": "An addon for Odyssey which provides a scrollytelling section type.",
  "license": "MIT",
  "private": true,
  "contributors": ["Nathan Hoad <nathan@nathanhoad.net>", "Simon Elvery <simon@elvery.net>"],
  "aunty": {
    "deploy": {
      "contentftp": {
        "files": "**",
        "from": "build",
        "to": "/www/res/sites/news-projects/<name>/<id>"
      }
    }
  },
  "scripts": {
    "start": "aunty serve"
  },
  "devDependencies": {
    "@abcnews/aunty": "^4.2.4"
  },
  "dependencies": {
    "preact": "^8.2.1",
    "preact-compat": "^3.16.0"
  }
}
